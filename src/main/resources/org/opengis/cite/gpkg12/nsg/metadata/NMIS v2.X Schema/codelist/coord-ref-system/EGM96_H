                        </gmd:EX_GeographicBoundingBox>
                    </gmd:geographicElement>
                </gmd:EX_Extent>
            </gml:domainOfValidity>
            <gml:scope>Used for vertical positioning worldwide.</gml:scope>
            <gml:anchorDefinition>WGS 84 ellipsoid.</gml:anchorDefinition>
        </gml:VerticalDatum>
    </gml:verticalDatum>
